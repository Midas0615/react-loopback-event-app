{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Account": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "login": true,
          "logout": true,
          "resetPassword": true,
          "prototype.__get__accessTokens": true
        }
      }
    }
  },
  "Contact": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "create": true,
          "prototype.patchAttributes": true,
          "deleteById": true,
          "sendEmail": true,
          "prototype.__get__events": true
        }
      }
    }
  },
  "Base": {
    "dataSource": "db",
    "public": false
  },
  "Event": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "create": true,
          "prototype.patchAttributes": true,
          "deleteById": true,
          "sendEmails": true,
          "sendEmail": true,
          "prototype.__get__contacts": true
        }
      }
    }
  },
  "Invitation": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "create": true,
          "deleteById": true,
          "prototype.patchAttributes": true,
          "confirm": true
        }
      }
    }
  },
  "ContactGroup": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "create": true,
          "prototype.patchAttributes": true,
          "deleteById": true,
          "prototype.__get__contacts": true,
          "sendEmails": true
        }
      }
    }
  },
  "Mailer": {
    "dataSource": "email",
    "public": false
  },
  "EmailTemplate": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "create": true,
          "prototype.patchAttributes": true,
          "deleteById": true,
          "prototype.__get__contacts": true
        }
      }
    }
  }
}
